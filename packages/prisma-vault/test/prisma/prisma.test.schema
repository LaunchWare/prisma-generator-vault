// prisma/prisma.test.schema
datasource db {
  provider = "sqlite"
  url      = "file:./db/test.db"
}

generator client {
  provider = "prisma-client-js"
}

generator prismaVault {
  provider = "node --enable-source-maps ../..//dist/packages/prisma-vault"
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  firstName  String
  lastName String
  posts Post[]
}

model Post {
  id     Int     @id @default(autoincrement())
  title  String
  body String
  authorId Int
  author User    @relation(fields: [authorId], references: [id])
}
